  "main": "src/index.jsx",
  "scripts": {
    "start": "npm run watch:copy & webpack --mode development --watch",
    "build": "npm run copy & webpack --mode production",
    "copy": "cp -r static dist/static && cp index.html dist/",
    "watch:copy": "nodemon --watch static --watch index.html --exec \"npm run copy\""
  },
